.note {
  background-color: #fff;
  border-radius: 30rpx;
  margin: 5px 15px 5px 15px !important
}

.noteContent {
  /* padding: 12px 20px 8px 20px; */
  padding: 30rpx 30rpx 20rpx 30rpx;
}

.noteTitle {
  font-size: 32rpx;
  font-weight: 600;
}

.noteDescription {
  font-size: 26rpx;
  margin-top: 20rpx;
  opacity: 0.6;
}

.noteCreateTime {
  margin-top: 20rpx;
  font-size: 24rpx;
  opacity: 0.6;
}

.addnote {
  background-image: url('data:image/png;base64,');
  width: 100rpx;
  height: 100rpx;
  position: fixed;
  right: 10%;
  top: 80%;
  background-size: 100%;
}

.floder {
  padding: 12px 12px 8px 12px;
  padding: 30rpx 30rpx 20rpx 30rpx;
}

.floderIcon {
  background-image: url(data:image/png;base64,);
  background-size: 100%;
  width: 64rpx;
  height: 64rpx;
  display: inline-block;
  background-repeat: no-repeat;
  background-position: center;
}

.floderTitle {
  display: inline-block;
  position: absolute;
  left: 20%;
  top: 50%;
  margin-top: -15rpx;
  font-size: 32rpx;
  font-weight: 600;
}

.floderNoteCount {
  display: inline-block;
  position: absolute;
  right: 10%;
  top: 50%;
  margin: -10rpx;
}